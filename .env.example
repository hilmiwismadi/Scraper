# VPS API Configuration
VPS_API_URL=https://sales.webbuild.arachnova.id/api
VPS_API_TOKEN=your_jwt_token_here

# Scraping Configuration
DEFAULT_START_INDEX=0
DEFAULT_END_INDEX=20

# Instagram Credentials (Optional - leave empty if not using)
INSTAGRAM_USERNAME=
INSTAGRAM_PASSWORD=

# LLM Configuration (Ollama)
# Set to 'true' to enable LLM-powered caption parsing
ENABLE_LLM=true
# Ollama host (default: http://localhost:11434)
OLLAMA_HOST=http://localhost:11434
# Model to use for parsing (gemma2:9b recommended for Indonesian text)
OLLAMA_MODEL=gemma2:9b
